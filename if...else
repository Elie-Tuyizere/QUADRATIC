import math
delta=float
a=int(input("inter a: "))
b=int(input("inter b: "))
c=int(input("inter c: "))
delta=(b*b)-(4*a*c)
if delta>0: 
    x1=float
    x2=float
    x1=(-b+math.sqrt(delta))/2*a
    x2=(-b-math.sqrt(delta))/2*a
    print("the value of x1 is: ", x1)
    print("the value of x2 is: ", x2)
elif delta<0:
    print("no solution")
else:
    x=float
    x=(-b)/2*a
    print("the value of x is: ",x)
